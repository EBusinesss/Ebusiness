package org.composants;

import org.entitees.Commande;

public class CmdAddImpl implements CmdAddInterface {

	public Commande ajouterCommande(int utilisateur_idUtilisateur,
			String montantCommande, String ligneCommande_idLigneCommande,
			int etatCommande) {
		
		Commande cmd = new Commande(utilisateur_idUtilisateur, montantCommande, ligneCommande_idLigneCommande, etatCommande);
		return cmd;
	}

	@Override
	public Commande ajouterCommande() {
		Commande cmd = new Commande();
		return cmd;
	}

}
