package org.dao;

import org.entitees.Categorie;

public interface CategorieInterface {
	
	public void creerCategie();
	
	public Categorie ajouterCategorie(String nom);
	
	public Categorie getCategorie(String nomCat);
	
    public void deleteAll();

    public java.util.Collection<com.unice.miage.igift.entity.Category> getCategoryList();

    public Category getCategoryByName(java.lang.String categoryName);

    public Category addCategory(java.lang.String name);

    public void createCategory();

}
