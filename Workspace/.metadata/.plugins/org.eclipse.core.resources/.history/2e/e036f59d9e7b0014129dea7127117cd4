package org.composants;

import java.util.Collection;
import java.util.Date;

import org.entitees.Commande;
import org.entitees.LigneCommande;
import org.entitees.Utilisateur;

public class CmdAddImpl implements CmdAddInterface {

	@Override
	public Commande ajouterCommande() {
		Commande cmd = new Commande();
		return cmd;
	}

	@Override
	public Commande ajouterCommande(Utilisateur client,
			Collection<LigneCommande> ligneCommande, Date dateCommande,
			String etatCommande, double prixTotalCommande) {
		Commande cmd = new Commande(client, ligneCommande, dateCommande, etatCommande, prixTotalCommande);
		return cmd;
	}

/*	public Commande ajouterCommande() {
		Commande cmd = new Commande(client, ligneCommande, dateCommande, etatCommande, prixTotalCommande);
		return cmd;
	}

	public Commande ajouterCommande() {
		Commande cmd = new Commande();
		return cmd;
	}*/

}
