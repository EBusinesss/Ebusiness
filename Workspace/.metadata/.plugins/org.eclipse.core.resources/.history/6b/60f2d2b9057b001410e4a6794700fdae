package org.commande;

import com.sun.xml.registry.common.tools.bindings_v3.Command;

public interface CmdInterface {
	
	public void ajouterCommande();
	public void ajouterCommande(Command cmd);
	public void modifierCommande();
	public void supprimerCommande();
	
	public void ajoutProduit();
	public void retirerProduit();
	public void modifierQuantiteeProduit();
	
	public void valideeCommande();

}
